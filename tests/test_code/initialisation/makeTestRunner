
# makefile to compile TestRunner.f90
#
# funit generated this file on 2019-09-17 23:43:00 +0100.

OBJ=initialisation_fun.o initialisation.o definedtypes.o constants.o dispersions.o TestRunner.o
LDFLAGS= -lstdc++

all:testrunner

testrunner: $(OBJ)
	gfortran-9   -o TestRunner $(OBJ) $(LDFLAGS)

TestRunner.o: TestRunner.f90 initialisation_fun.o 
	(cd .; gfortran-9  -I. -c TestRunner.f90)
initialisation_fun.o: initialisation_fun.f90 initialisation.o definedtypes.o 
	(cd .; gfortran-9  -I. -c initialisation_fun.f90)
initialisation.o: initialisation.f90 constants.o dispersions.o definedtypes.o 
	(cd .; gfortran-9  -I. -c initialisation.f90)
constants.o: constants.f90  
	(cd .; gfortran-9  -I. -c constants.f90)
dispersions.o: dispersions.f90 constants.o 
	(cd .; gfortran-9  -I. -c dispersions.f90)
definedtypes.o: definedtypes.f90 constants.o 
	(cd .; gfortran-9  -I. -c definedtypes.f90)
  
